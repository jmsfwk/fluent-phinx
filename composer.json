{
    "name": "jmsfwk/fluent-phinx",
    "description": "Laravel style migrations with Phinx",
    "type": "library",
    "keywords": ["Laravel", "Phinx", "migrations"],
    "authors": [
        {
            "name": "James Fenwick",
            "email": "j.fenwick@me.com"
        }
    ],
    "license": "proprietary",
    "require": {
        "robmorgan/phinx": "^0.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "jmsfwk\\FluentPhinx\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\FluentPhinx\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
